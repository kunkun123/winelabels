<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>demo-PubEnttype</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="../../../static/styles/css/ace-fonts.css"/>
    <link rel="stylesheet" type="text/css" href="../../../static/styles/css/font-awesome.css"/>
    <link rel="stylesheet" type="text/css" href="../../../static/styles/css/bootstrap.css"/>
    <link rel="stylesheet" type="text/css" href="../../../static/styles/css/bootstrap-table.min.css"/>
    <link rel="stylesheet" href="../../../static/scripts/gecko-modules/bootstrap/bootstrap-editable.css">
    <link rel="stylesheet" type="text/css" href="../../../static/styles/css/ace.css"/>
    <link rel="stylesheet" type="text/css" href="../../../static/styles/gecko.css"/>
    <script type="text/javascript" src="../../../static/scripts/gecko-modules/jquery/jquery.js"></script>
    <script type="text/javascript" src="../../../static/scripts/gecko-modules/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="../../../static/scripts/gecko-modules/bootstrap/bootstrap-table.js"></script>
    <script src="../../../static/scripts/gecko-modules/bootstrap/bootstrap-table-editable.js"></script>
    <script src="../../../static/scripts/gecko-modules/bootstrap/bootstrap-editable.min.js"></script>
    <script type="text/javascript" src="../../../static/scripts/gecko-modules/bootstrap/bootstrap-table-zh-CN.js"></script>
    <script type="text/javascript" src="../../../static/scripts/app/system/comm/utils.js"></script>
    <script type="text/javascript" src="../../../static/scripts/demo/pubenttype/pubenttype.js"></script>

</head>
<body>
<!--顶部按钮-->
<div id="toolbar">
    <div class="btn-group">
        <button class="btn btn-md btn-success" role="button" data-toggle="modal" data-target="#myModal">
            <i class="ace-icon fa fa-plus-square-o bigger-110"></i>
            新建
        </button>
        <button class="btn btn-md btn-primary" onclick="detailShow();">

            明细
        </button>
        <button id="delete" class="btn btn-md btn-danger" onclick="remove();">删除</button>
    </div>
</div>

<div class="page-content">
    <span id="user_msg"></span>
    <table id="pubenttypeTable"></table>
</div>

<!-- Moda窗口 -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <i class="normal-icon ace-icon fa fa-user red bigger-130 user-icon"></i>
                <span class="modal-title" style="color: #DD5A43">新增企业类型</span>
            </div>
            <!--modal-body部分-->
            <div class="modal-body">
                <form id="enttypeForm" class="form-inline">
                    <input type="hidden" id="id" name="id" value=""/>

                    <input type="hidden" id="parententtype" name="parententtype" value=""/>

                    <input type="hidden" id="creator" name="creator" value=""/>
                    <div class="form-group">
                        <label for="enttypenum">企业类型编号</label>
                        <input type="text" class="form-control" id="enttypenum" name="enttypenum" placeholder="企业类型编号">
                    </div>
                    <div class="form-group">
                        <label for="enttypename">企业类型名称</label>
                        <input type="text" class="form-control" id="enttypename" name="enttypename" placeholder="企业类型名称">
                    </div>

                    <div class="form-group">
                        <label for="sourceid">资源编号</label>
                        <input type="text" class="form-control" id="sourceid" name="sourceid" placeholder="资源编号">
                    </div>

                </form>
            </div>
            <!--modal-footer部分-->
            <div class="modal-footer">
                <button type="button" class="btn btn-primary save" onclick="add()">保存</button>
            </div>
        </div>
    </div>
</div>

<!-- 明细Moda窗口 -->
<div class="modal fade" id="myDetailModal" tabindex="-1" role="dialog" aria-labelledby="myDetailModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <i class="normal-icon ace-icon fa fa-user red bigger-130 user-icon"></i>
                <span class="modal-title" style="color: #DD5A43">企业类型明细</span>
            </div>
            <!--modal-body部分-->
            <div class="modal-body">
                <form id="enttypeFormDetail" class="form-inline">


                    <div class="form-group">
                        <label for="enttypenumShow">企业类型编号</label>
                        <input type="text" class="form-control" id="enttypenumShow" name="enttypenum" readonly="readonly">
                    </div>
                    <div class="form-group">
                        <label for="enttypenameShow">企业类型名称</label>
                        <input type="text" class="form-control" id="enttypenameShow" name="enttypename" readonly="readonly">
                    </div>

                    <div class="form-group">
                        <label for="sourceidShow">资源编号</label>
                        <input type="text" class="form-control" id="sourceidShow" name="sourceid" readonly="readonly">
                    </div>
                </form>

            </div>
            <!--modal-footer部分-->
            <div class="modal-footer">
                <button type="button" class="btn btn-primary save" onclick="$('#myDetailModal').modal('hide');">关闭</button>
            </div>
        </div>
    </div>
</div>
</body>
</html>