<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.gecko.framework.trusteeship.repository.dao.MgTrusteeshipDao">
	<select id="findByEventId" resultMap="MgTrusteeship" parameterType="java.math.BigDecimal">
        select m.* from MG_TRUSTEESHIP m where m.eventId = #{eventId}
    </select>
    <select id="findByCustodian" resultMap="MgTrusteeship" parameterType="String">
        select m.* from MG_TRUSTEESHIP m where m.custodian = #{custodian}
    </select>
     <select id="findByTrustee" resultMap="MgTrusteeship" parameterType="String">
        select m.* from MG_TRUSTEESHIP m where m.trustee = #{trustee}
    </select>
</mapper>