 <script type="text/javascript">  
        $(window).on('load', function () {  
  
            $('.selectpicker').selectpicker({  
                'selectedText': 'cat'
            });
            $('.selectpicker').selectpicker('setStyle', 'btn-white');
  
            // $('.selectpicker').selectpicker('hide');  
        });  
    </script>  
<script type="text/javascript">
$(function () {
		/**
		*	查询单位加载所有的部门
		*/
		$.ajax({
			url: "/system/organization/list", 
			type: "POST", 
			success: function(data){
				$.each(data.rows, function(i,item){
				$("#departmentId").append("<option value='"+item.id+"'>"+item.orgName+"</option>");  
				});
			}
		});
		if(!ace.vars['old_ie']) $("input[id$='Date']").datepicker({
				 //format: 'YYYY/MM/DD h:mm:ss A',//use this option to display seconds
				 	format: 'yyyy-MM-dd',  
		        	language: 'cn',  
		        	pickDate: true,  
		        	pickTime: true,  
		       		hourStep: 1,  
		       		todayBtn : true,
		        	minuteStep: 1,  
		        	secondStep: 30,  
		        	inputMask: true  
				});
	
	$("#taskList").bootstrapTable({
                //toolbar: "#toolbar",
                idField: "id",
                pagination: true,
			    sidePagination: 'client',
			    pageNumber: 1,
			    pageSize: 5,
			    pageList: [5, 10, 20],
                url: "/complaint/cpcomplaintsreport/complaintsList",
                queryParams:queryParams,
                columns: [  
                [{
                         "title": "<img src='/static/scripts/gecko-modules/img/u152.png' class='img-rounded' >案件列表",
                         "halign":"left",
                         "align":"center",
                         "colspan": 15
                }],
                [{
                    checkbox: true
                }, {
                    field: "id",
                    visible: false
                },{
                    field: "cpComplaintsreportId",
                    title: "信息编号",
                    type: "text",
                    search: "true",
                     sortable: true,
                    editable: {
                        type: "text"
                    }
                }, {
                    field: "source",
                    title: "业务来源",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                }, {
                    field: "assignee",
                    title: "受理员",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                },{
                    field: "businessType",
                    title: "业务类型",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                },{
                    field: "acceptype",
                    title: "接收方式",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                },{
                    field: "content",
                    title: "反应内容",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                }, {
                    field: "registerdate",
                    title: "登记时间",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                },{
                    field: "limitdate",
                    title: "办结期限",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                },{
                    field: "status",
                    title: "当前状态",
                    type: "text",
                    search: "true",
                    editable: {
                        type: "text"
                    }
                }, {
                    field: 'operate',
                	title: '操作',
                	align: 'center',
                	width : 100,
                	//events: operateEvents,
                	formatter: operateFormatter
                }]]
            });
     	
});
	function operateFormatter(value, row, index) {
      return [
      '<button id="oleOfedit" onclick="javascript:a('+row.cpComplaintsreportId+','+row.id+');" type="button" class="oleOfedit btn btn-primary  btn-sm" style="margin-right:15px;">督办</button>'
      //,'<button id="oleOfedit" onclick="javascript:a('+row.id+');" type="button" class="oleOfedit btn btn-primary  btn-sm" style="margin-right:15px;">查看当前流程图</button>'
      ].join('');
	};
	window.operateEvents = {
	    'click .oleOfedit': function (e, value, row, index) {
	    	33();
	    	alert(111);
	    	
	    	console.log(row);
	    	//window.location.href="/workflow/startProcess?id="+row.id; 
		}
	};
	function a(cpComplaintsreportId,taskId){
		window.location.href = "/complaint/cpcomplaintsreport/superviseView?cpComplaintsreportId="+cpComplaintsreportId+"&&taskId="+taskId;
	}
	function b(taskId){
		window.location.href = "/workflow/complaint/autio?taskId="+taskId;
	}
	function queryParams(params) {
	    var param = {
	    	isexceed : $("#isexceed").find("option:selected").val(),
	    	queryLevel : $("#queryLevel").find("option:selected").val(),
	    	departmentId : $("#departmentId").find("option:selected").val(),
	    	currentStatus : $("#currentStatus").find("option:selected").val(),
	    	feedbackStatus : $("#feedbackStatus").val(),
	        source : $("#source").val(),
	        businessType : $("#businessType").val(),
	        startDate : $("#startDate").val(),
	        endDate : $("#endDate").val(),
	        acceptype : $("#acceptype").val(),
	        operator : $("#operator").val(),
	        infoid : $("#infoid").val(),
	        content : $("#content").val(),
	        orderMethod:$("input[name='orderMethod']:checked").val(),
	        orderType:$("input[name='orderType']:checked").val()
	    }
	    return param;
	} 
	function query(){
	  $('#taskList').bootstrapTable('refresh');  
	}
	function getMoreQuery(){
		$("[name='moreCondition']").removeClass("moreCondition");
		$("#moreQuery").hide();
		$("#lessQuery").show();
	}
	function getlessQuery(){
		$("[name='moreCondition']").addClass("moreCondition");
		$("#lessQuery").hide();
		$("#moreQuery").show();
	}
	function clear1(){
		//alert(11);
		document.getElementById("queryForm").reset();
	}
</script>